   "source": [
    "from __future__ import division\n",
    "\n",
    "if __name__=='__main__':\n",
    "    a = int(input())\n",
    "    b = int(input())\n",
    "    if b == 0:\n",
    "        raise ZeroDivisionError('Please select a non zero number!')\n",
    "    print(a//b)\n",
    "    print(a%b)\n",
    "    print(divmod(a,b))\n"
   ]
