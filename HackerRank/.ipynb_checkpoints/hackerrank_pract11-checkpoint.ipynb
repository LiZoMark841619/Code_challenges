   "source": [
    "from itertools import product\n",
    "\n",
    "def cartesian(A, B):\n",
    "    good_list = list((product(A, B)))\n",
    "    print(*good_list, sep = \" \")\n",
    "\n",
    "if __name__=='__main__':\n",
    "    arr1 = map(int, input().strip().split())\n",
    "    arr2 = map(int, input().strip().split())\n",
    "    cartesian(arr1, arr2)"
   ]
