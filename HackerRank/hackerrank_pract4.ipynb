    "def split_and_join(line):\n",
    "    line = line.split(' ')\n",
    "    return '-'.join(line)\n",
    "        \n",
    "if __name__ == '__main__':\n",
    "    line = input('Please enter a line:')\n",
    "    result = split_and_join(line)\n",
    "    print(result)"
